     code : |1-
      PROCEDURE WIN_AddModel(gnOpeningType is int = CN_ADD, gsModelName is string = "")
      
      // Modification
      IF gnOpeningType = CN_MODIFY THEN
      	
      	// Stores the template name
      	EDT_MODEL_NAME = gsModelName
      	
      END
      
      // Default return value
      MyWindow..ReturnedValue = False
