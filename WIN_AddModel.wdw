        internal_properties : CAAAAAgAAAAVBoz9GyLxV/cpZAP/ixHth4ZTK1Vu3D9NXK+sXV2BTZ1dCS8vV3emiqd4wQzfixWNSYyccUGkzaiG+91Vnkhz1s0z8q12+B84QW9aoZy+G8kj6T/4yE5f/Cr2ewxq1mHaUE+OL0iN
        code : |1-
         
         IF EDT_MODEL_NAME ~= "" THEN
         	Error(<§$0000§>)
         	ReturnToCapture(EDT_MODEL_NAME)
         END
         
         // Depending on the opening type (modification or addition)
         IF gnOpeningType = CN_MODIFY THEN
         	
         	// Replace the former name of the model by the new name
         	pGlobal.gsListModels = Replace(pGlobal.gsListModels, gsModelName, EDT_MODEL_NAME, WholeWord)
         	
         ELSE
         	
         	// Add the template to the list of preset templates
         	pGlobal.gsListModels += EDT_MODEL_NAME + CR
         	
         END
         
         // The current model is the model that was added
         pGlobal.gsNameCurrentModel = EDT_MODEL_NAME
         
         MyWindow..ReturnedValue = True
